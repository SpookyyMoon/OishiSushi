<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFF">

    <ImageView
        android:id="@+id/atras"
        android:layout_width="27dp"
        android:layout_height="17dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="339dp"
        android:layout_marginBottom="778dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/atras" />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="155dp"
        android:layout_height="111dp"
        android:layout_marginTop="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <ImageView
        android:id="@+id/carrito"
        android:layout_width="38dp"
        android:layout_height="21dp"
        android:layout_marginStart="339dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="778dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/carrito" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="305dp"
        android:layout_height="193dp"
        android:layout_marginStart="43dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="571dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.848"
        app:srcCompat="@drawable/group_8" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="162dp"
        android:layout_marginBottom="659dp"
        android:fontFamily="@font/poppins"
        android:text="Nuestras promociones"
        android:textColor="#F8F8F8"
        android:textSize="18sp"
        android:typeface="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView9"
        app:layout_constraintHorizontal_bias="0.252"
        app:layout_constraintStart_toStartOf="@+id/imageView9"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/button"
        android:layout_width="80dp"
        android:layout_height="21dp"
        android:layout_marginStart="144dp"
        android:layout_marginTop="197dp"
        android:layout_marginEnd="169dp"
        android:layout_marginBottom="633dp"
        android:backgroundTint="#FF9C80"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        android:textAlignment="center"
        android:textSize="8sp"
        android:translationX="-20dp"
        app:iconPadding="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ver Promociones"
        android:textColor="#FFFFFF"
        android:textSize="8sp"
        android:translationX="-20dp"
        app:layout_constraintBottom_toBottomOf="@+id/button"
        app:layout_constraintEnd_toEndOf="@+id/button"
        app:layout_constraintStart_toStartOf="@+id/button"
        app:layout_constraintTop_toTopOf="@+id/button" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="305dp"
        android:layout_height="68dp"
        android:layout_marginStart="43dp"
        android:layout_marginTop="218dp"
        android:layout_marginEnd="45dp"
        android:layout_marginBottom="565dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/busqueda" />

    <androidx.appcompat.widget.SearchView
        android:id="@+id/searchView"
        android:layout_width="294dp"
        android:layout_height="19dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:background="@android:color/transparent"
        android:paddingStart="-15dp"
        app:iconifiedByDefault="false"
        app:layout_constraintBottom_toBottomOf="@+id/imageView12"
        app:layout_constraintEnd_toEndOf="@+id/imageView12"
        app:layout_constraintStart_toStartOf="@+id/imageView12"
        app:layout_constraintTop_toTopOf="@+id/imageView12"
        app:queryBackground="@android:color/transparent"
        app:searchIcon="@drawable/iconobuscar" />

    <FrameLayout
        android:id="@+id/botonCategoriaRolls"
        android:layout_width="64dp"
        android:layout_height="34dp"
        android:layout_marginStart="164dp"
        android:layout_marginTop="276dp"
        android:layout_marginEnd="165dp"
        android:layout_marginBottom="541dp"
        android:background="@drawable/boton_categoria"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <FrameLayout
        android:id="@+id/botonCategoriaRamen"
        android:layout_width="64dp"
        android:layout_height="34dp"
        android:layout_marginStart="235dp"
        android:layout_marginTop="276dp"
        android:layout_marginEnd="94dp"
        android:layout_marginBottom="541dp"
        android:background="@drawable/boton_categoria"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <FrameLayout
        android:id="@+id/botonCategoriaSushi"
        android:layout_width="64dp"
        android:layout_height="34dp"
        android:layout_marginStart="93dp"
        android:layout_marginTop="276dp"
        android:layout_marginEnd="236dp"
        android:layout_marginBottom="541dp"
        android:background="@drawable/boton_categoria"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="37dp"
        android:layout_height="22dp"
        android:layout_marginStart="75dp"
        android:layout_marginEnd="259dp"
        android:translationX="5dp"
        android:translationY="-4dp"
        app:layout_constraintBottom_toBottomOf="@+id/botonCategoriaSushi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/botonCategoriaSushi"
        app:layout_constraintVertical_bias="0.833"
        app:srcCompat="@drawable/sushiicono" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="37dp"
        android:layout_height="22dp"
        android:layout_marginStart="145dp"
        android:layout_marginEnd="189dp"
        android:translationX="5dp"
        android:translationY="-3dp"
        app:layout_constraintBottom_toBottomOf="@+id/botonCategoriaRolls"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/botonCategoriaRolls"
        app:layout_constraintVertical_bias="0.75"
        app:srcCompat="@drawable/rollsicono" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="27dp"
        android:layout_height="18dp"
        android:layout_marginStart="215dp"
        android:layout_marginEnd="126dp"
        android:translationX="6dp"
        android:translationY="-2.5dp"
        app:layout_constraintBottom_toBottomOf="@+id/botonCategoriaRamen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/botonCategoriaRamen"
        app:layout_constraintVertical_bias="0.625"
        app:srcCompat="@drawable/ramenicono" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="124dp"
        android:layout_marginEnd="244dp"
        android:text="Sushi"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/botonCategoriaSushi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/botonCategoriaSushi" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="196dp"
        android:layout_marginEnd="168dp"
        android:text="Rolls"
        android:textSize="10sp"
        android:translationX="-3dp"
        app:layout_constraintBottom_toBottomOf="@+id/botonCategoriaRolls"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/botonCategoriaRolls" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="260dp"
        android:layout_marginEnd="101dp"
        android:text="Ramen"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/botonCategoriaRamen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/botonCategoriaRamen" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/contenedorPlatos"
        android:layout_width="328dp"
        android:layout_height="512dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="321dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>